{
	"name": "",
	"version": "1.0.0",
	"description": "This is a simple template using Webpack to bundle.",
	"main": "index.html",
	"scripts": {
		"start": "webpack-dev-server --config webpack.config.js --mode=development",
		"build": "webpack --config webpack.config.js --mode=production",
		"start:dist": "http-server dist",
		"docker": "npm run build && docker build . -t ff0000/ad-red-production:latest",
		"docker:triton": "docker push ff0000/ad-red-production && triton-docker pull ff0000/ad-red-production",
		"triton:run": "triton-docker run -d -p 80:80 -p 443:443 ff0000/ad-red-production:latest"
	},
